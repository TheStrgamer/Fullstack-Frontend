{
  "name": "fullstack-frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lmao": "vite --host",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview --port 5173 --host --port 5173 --host",
    "test:unit": "vitest",
    "prepare": "cypress install",
    "test:e2e": "start-server-and-test preview http://localhost:5173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 5173' http://localhost:5173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "lodash": "^4.17.21",
    "pinia": "^3.0.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.14.0",
    "@cypress/vue": "^6.0.2",
    "@cypress/webpack-preprocessor": "^6.0.4",
    "@faker-js/faker": "^9.6.0",
    "@pinia/testing": "^1.0.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.9",
    "@types/vue": "^1.0.31",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/coverage-istanbul": "^3.1.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cypress": "^14.3.0",
    "flush-promises": "^1.0.2",
    "istanbul-lib-coverage": "^3.2.2",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "3.5.3",
    "start-server-and-test": "^2.0.10",
    "ts-loader": "^9.5.2",
    "typescript": "~5.8.0",
    "vite": "^6.2.3",
    "vite-plugin-istanbul": "^7.0.0",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.0.8",
    "vue-tsc": "^2.2.8"
  }
}
